                <head xml:id="docIn12">Tekstkritik</head>
                <list type="textualCriticismZero"/>
                
            </div> 
            
        
                    <div type="litList">
            <head xml:id="docIn100">Anvendt litteratur</head>
                <!-- <div>  
                <head xml:id="docIn101">Trykt</head>-->
            
                <list type="litList">
                
