            <note xml:id="com1"><!--lemma: saa-->
                <p>et sandf&#x00E6;rdigt historiev&#x00E6;rk.</p>
            </note>
          
            <note xml:id="com23" changes="new"><!--lemma: lovedes-->
                <p>blev lovprist.</p>
            </note>
          
            <note xml:id="com2"><!--lemma: saa-->
                <p>s&#x00E5;dan.</p>
            </note>
       
            <note xml:id="com28" changes="new"><!--lemma: Aarbog-->
                <p>annal, dvs. bog med fortegnelse over vigtige begivenheder, opstillet kronologisk &#x00E5;r for &#x00E5;r.</p>
            </note>
       
            <note xml:id="com20" changes="new"><!--lemma: talen om-->
                <p>omtalen af.</p>
            </note>
            
            <note xml:id="com21" changes="new"><!--lemma: genkalde-->
                <p>tage i dig igen, tilbagekalde.</p>
            </note>            
           
            <note xml:id="com24" changes="new"><!--lemma: Pag.-->
                <p>side.</p>
            </note>
           
            <note xml:id="com3"><!--lemma: saa-->
                <p>s&#x00E5;ledes.</p>
            </note>
            
            <note xml:id="com29" changes="new"><!--lemma: sværmerie-->
                <p>religi&#x00F8;s betagelse p&#x00E5; gr&#x00E6;nsen til fanatisme.</p>
            </note>
            
            <note xml:id="com4"><!--lemma: vist mærke-->
                <p>sikkert kendetegn.</p>
            </note> 
            
            <note xml:id="com5"><!--lemma: de Hellige Skribentere-->
                <p>de bibelske forfattere.</p>
            </note>
            
            <note xml:id="com25" changes="new"><!--lemma: rigtig-->
                <p>rigtignok, sandelig.</p><!-- 'rigtignok' har også indrømmende betydning på nudansk og derfor måske ikke så præcist? -->
            </note>
            <!-- ODS 9.1 -->
            
            <note xml:id="com26" changes="new"><!--lemma: forskrevet mig-->
                <p>skrevet fejl.</p>
            </note><!-- ODS 3.1 -->
            
            <note xml:id="com6"><!--lemma: kiender du mig for-->
                <p>opfatter du mig som.</p>
                <!-- ODS 5.1 -->
            </note>
           
            <note xml:id="com7"><!--lemma: rigtig-->
                <p>i h&#x00F8;j grad.</p>
            </note>
                       
            <note xml:id="com8"><!--lemma: falde paa-->
                <p>f&#x00E5; indtryk af.</p>
                <!-- egentlig: 'Den idé/tanke, kunne folk nemt gribes af', se ODS 5.3, men den fungerer ikke syntaktisk, så skal hele sætningen lemmatiseres. -->
            </note>           
           
            <note xml:id="com9"><!--lemma: fløtte-->
                <p>flytter, dvs. d&#x00F8;r.</p>
            </note>
            
            <note xml:id="com10"><!--lemma: Haandklap-->
                <p>bifald.</p>
            </note>
            
            <note xml:id="com11"><!--lemma: give Gud-->
                <p>m&#x00E5; Gud give (optativ).</p>
            </note>
            
            <note xml:id="com27"><!--lemma: lod-->
                <p>s&#x00E5; ud.</p>
            </note>
            
            <note xml:id="com12"><!--lemma: som-->
                <p>som om.</p>
            </note>
            
            <note xml:id="com13"><!--lemma: Begiæring-->
                <p>beg&#x00E6;r.</p>
            </note>           
            
            <note xml:id="com14"><!--lemma: vorde-->
                <p>m&#x00E5; blive (optativ).</p>
            </note>
            
            <!-- kunne overvej com på 'skrive dig således til', men det er kun et spm om ordstilling: 'skrive således til dig' -->
                                    
            <note xml:id="com15"><!--lemma: lade-->
                <p>se ud.</p>
            </note>
            
            <note xml:id="com16"><!--lemma: som-->
                <p>som om.</p>
            </note>
            
            <note xml:id="com17"><!--lemma: enddog-->
                <p>selv om.</p>
            </note>
            
